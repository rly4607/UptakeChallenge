<!-- AdminLTE App -->
<script src="dist/js/app.min.js"></script>

<script>
  $(function() {
    $(".button").click(function() {

      var answer = $('#stage1').val();

      $.ajax({
       type: "POST",
       url: "solve",
       dataType : 'json',
       data: { stage1: answer },
       success: function(data){ 

        if ( data['stage_solved'] == true) {
           window.location.href = "https://y3vyaw9zaxr5.uptake.com/secure-login/portal/stage2" 
      } 

      if ( data['stage_solved'] == false ) {
        sweetAlert("T_R_U_S_T is invalid", "The cookie does not exist", "error");
      } 
      
      if ( data['stage_solved'] == "already_solved" ) {
        //already solved
      }
    }
  });

    });
  });

</script>
